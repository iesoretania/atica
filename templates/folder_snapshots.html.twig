{% extends "tree.html.twig" %}
{% block maincontent %}
<div class="panel panel-info">
    <div class="panel-heading">
        <h1>Entregas anteriores de la carpeta</h1>
    </div>
</div>
{% for snapshot in snapshots %}
    {% set data = snapshot.data %}
    {% if data %}
    <div class="panel panel-default">
        {% set top_buttons = true %}
        {% set no_manager = true %}
        {% set no_upload = true %}
        {% if data %}
        {% include 'folder.html.twig' %}
        {% endif %}
        <div class="panel-footer">
            <a class="btn btn-info" href="{{ last_url }}">Volver</a>
        </div>
    </div>
    {% endif %}
{% endfor %}
{% endblock %}
